<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="tp_aa.Task" table="Task" lazy="false">
		<id name="id" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="name" column="Name" type="string" length="255" not-null="true" lazy="false"/>
		<set name="ORM_Subtaks" lazy="true" cascade="save-update,lock" inverse="false">
			<key column="TaskID" not-null="true"/>
			<one-to-many class="tp_aa.SubTask"/>
		</set>
	</class>
</hibernate-mapping>
