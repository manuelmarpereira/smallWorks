<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="hibernate.subscription.subscription" table="subscription" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<many-to-one name="user" cascade="lock" column="UserID" class="hibernate.user.User" not-null="true" lazy="proxy" access="field">
		</many-to-one>
		<many-to-one name="subsTask" cascade="lock" column="TaskID" class="hibernate.task.Task" not-null="true" lazy="proxy" access="field">
		</many-to-one>
		<many-to-one name="subsDistrict" cascade="lock" column="DistrictID" class="hibernate.district.District" not-null="true" lazy="proxy" access="field">
		</many-to-one>
	</class>
</hibernate-mapping>
